{
    "root": true,
    "plugins": ["prettier", "import", "@typescript-eslint"],
    "extends": ["eslint:recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "import/extensions": "off",
        "import/prefer-default-export": "off",
        "no-debugger": "off",
        "no-console": 0,
        "class-methods-use-this": "off",
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ]
    }
}
